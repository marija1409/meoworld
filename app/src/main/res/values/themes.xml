<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Theme.Meoworld" parent="android:Theme.Material.Light.NoActionBar">
        <item name="android:windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
    </style>


    <style name="Theme.Meoworld.Splash" parent="Theme.SplashScreen">
        <item name="windowSplashScreenBackground">#FFFFFF</item>
        <item name="windowSplashScreenAnimatedIcon">@mipmap/splash_icon</item>
        <item name="windowSplashScreenIconBackgroundColor">#FFFFFF</item>
        <item name="postSplashScreenTheme">@style/Theme.Meoworld</item>

        <item name="android:windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>

    </style>

</resources>